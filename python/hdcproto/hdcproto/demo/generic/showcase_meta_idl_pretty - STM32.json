{
    "version": "HDC 1.0.0-alpha.11",
    "max_req": 128,
    "features": [
        {
            "id": 0,
            "name": "core",
            "cls": "MinimalCore",
            "version": "0.0.1",
            "doc": "STM32 C implementation of the 'Minimal' HDC-device demonstration",
            "states": [
                {
                    "id": 0,
                    "name": "OFF"
                },
                {
                    "id": 1,
                    "name": "INIT"
                },
                {
                    "id": 2,
                    "name": "READY"
                },
                {
                    "id": 255,
                    "name": "ERROR"
                }
            ],
            "commands": [
                {
                    "id": 1,
                    "name": "reset",
                    "doc": "Reinitializes the whole device.",
                    "args": [],
                    "returns": [],
                    "raises": []
                },
                {
                    "id": 2,
                    "name": "division",
                    "doc": "Divides numerator by denominator.",
                    "args": [
                        {
                            "dtype": "FLOAT",
                            "name": "numerator"
                        },
                        {
                            "dtype": "FLOAT",
                            "name": "denominator",
                            "doc": "Beware of the zero!"
                        }
                    ],
                    "returns": [
                        {
                            "dtype": "DOUBLE",
                            "doc": "Quotient of numerator/denominator"
                        }
                    ],
                    "raises": [
                        {
                            "id": 1,
                            "name": "MyDivZero"
                        }
                    ]
                },
                {
                    "id": 240,
                    "name": "get_property_value",
                    "args": [
                        {
                            "dtype": "UINT8",
                            "name": "property_id"
                        }
                    ],
                    "returns": [
                        {
                            "dtype": "BLOB",
                            "doc": "Actual data-type depends on property"
                        }
                    ],
                    "raises": [
                        {
                            "id": 245,
                            "name": "UnknownProperty"
                        }
                    ]
                },
                {
                    "id": 241,
                    "name": "set_property_value",
                    "args": [
                        {
                            "dtype": "UINT8",
                            "name": "property_id"
                        },
                        {
                            "dtype": "BLOB",
                            "name": "new_value",
                            "doc": "Actual data-type depends on property"
                        }
                    ],
                    "returns": [
                        {
                            "dtype": "BLOB",
                            "name": "actual_new_value",
                            "doc": "May differ from NewValue!"
                        }
                    ],
                    "raises": [
                        {
                            "id": 245,
                            "name": "UnknownProperty"
                        },
                        {
                            "id": 246,
                            "name": "ReadOnlyProperty"
                        }
                    ]
                }
            ],
            "events": [
                {
                    "id": 1,
                    "name": "button",
                    "doc": "Notify host about the button being pressed on the device.",
                    "args": [
                        {
                            "dtype": "UINT8",
                            "name": "ButtonID"
                        },
                        {
                            "dtype": "UINT8",
                            "name": "ButtonState"
                        }
                    ]
                },
                {
                    "id": 240,
                    "name": "log",
                    "doc": "Forwards software event log to the host.",
                    "args": [
                        {
                            "dtype": "UINT8",
                            "name": "log_level",
                            "doc": "Same as in Python"
                        },
                        {
                            "dtype": "UTF8",
                            "name": "log_msg"
                        }
                    ]
                },
                {
                    "id": 241,
                    "name": "feature_state_transition",
                    "doc": "Notifies host about transitions of this feature's state-machine.",
                    "args": [
                        {
                            "dtype": "UINT8",
                            "name": "previous_state_id"
                        },
                        {
                            "dtype": "UINT8",
                            "name": "current_state_id"
                        }
                    ]
                }
            ],
            "properties": [
                {
                    "id": 16,
                    "name": "uc_devid",
                    "dtype": "UINT32",
                    "ro": true,
                    "doc": "32bit Device-ID of STM32 microcontroller."
                },
                {
                    "id": 17,
                    "name": "uc_uid",
                    "dtype": "BLOB",
                    "size": 12,
                    "ro": true,
                    "doc": "96bit unique-ID of STM32 microcontroller."
                },
                {
                    "id": 18,
                    "name": "led_blinking_rate",
                    "dtype": "UINT8",
                    "ro": false,
                    "doc": "Blinking frequency of the LED given in Herz."
                },
                {
                    "id": 240,
                    "name": "log_event_threshold",
                    "dtype": "UINT8",
                    "ro": false,
                    "doc": "Suppresses LogEvents with lower log-levels."
                },
                {
                    "id": 241,
                    "name": "feature_state",
                    "dtype": "UINT8",
                    "ro": true,
                    "doc": "Current feature-state"
                }
            ]
        }
    ]
}