{
    "version": "HDC 1.0.0-alpha.11",
    "max_req": 128,
    "features": [
        {
            "id": 0,
            "name": "Core",
            "cls": "MinimalCore",
            "version": "0.0.1",
            "doc": "Python implementation of the 'Minimal' HDC-device demonstration",
            "states": [
                {
                    "id": 0,
                    "name": "OFF"
                },
                {
                    "id": 1,
                    "name": "INIT"
                },
                {
                    "id": 2,
                    "name": "READY"
                },
                {
                    "id": 255,
                    "name": "ERROR"
                }
            ],
            "commands": [
                {
                    "id": 1,
                    "name": "Reset",
                    "doc": "Reinitializes the whole device.",
                    "args": [],
                    "returns": [],
                    "raises": []
                },
                {
                    "id": 2,
                    "name": "Divide",
                    "doc": "Divides numerator by denominator.",
                    "args": [
                        {
                            "dtype": "FLOAT",
                            "name": "numerator"
                        },
                        {
                            "dtype": "FLOAT",
                            "name": "denominator",
                            "doc": "Beware of the zero!"
                        }
                    ],
                    "returns": [
                        {
                            "dtype": "DOUBLE",
                            "doc": "Quotient of numerator/denominator"
                        }
                    ],
                    "raises": [
                        {
                            "id": 1,
                            "name": "MyDivZero"
                        }
                    ]
                },
                {
                    "id": 240,
                    "name": "GetPropertyValue",
                    "args": [
                        {
                            "dtype": "UINT8",
                            "name": "PropertyID"
                        }
                    ],
                    "returns": [
                        {
                            "dtype": "BLOB",
                            "doc": "Actual data-type depends on property"
                        }
                    ],
                    "raises": [
                        {
                            "id": 245,
                            "name": "UnknownProperty"
                        }
                    ]
                },
                {
                    "id": 241,
                    "name": "SetPropertyValue",
                    "args": [
                        {
                            "dtype": "UINT8",
                            "name": "PropertyID"
                        },
                        {
                            "dtype": "BLOB",
                            "name": "NewValue",
                            "doc": "Actual data-type depends on property"
                        }
                    ],
                    "returns": [
                        {
                            "dtype": "BLOB",
                            "name": "ActualNewValue",
                            "doc": "May differ from NewValue!"
                        }
                    ],
                    "raises": [
                        {
                            "id": 245,
                            "name": "UnknownProperty"
                        },
                        {
                            "id": 246,
                            "name": "ReadOnlyProperty"
                        }
                    ]
                }
            ],
            "events": [
                {
                    "id": 1,
                    "name": "ButtonEvent",
                    "doc": "Notify host about the button being pressed on the device.",
                    "args": [
                        {
                            "dtype": "UINT8",
                            "name": "ButtonID"
                        },
                        {
                            "dtype": "UINT8",
                            "name": "ButtonState"
                        }
                    ]
                },
                {
                    "id": 240,
                    "name": "Log",
                    "doc": "Forwards software event log to the host.",
                    "args": [
                        {
                            "dtype": "UINT8",
                            "name": "LogLevel",
                            "doc": "Same as in Python"
                        },
                        {
                            "dtype": "UTF8",
                            "name": "LogMsg"
                        }
                    ]
                },
                {
                    "id": 241,
                    "name": "FeatureStateTransition",
                    "doc": "Notifies host about transitions of this feature's state-machine.",
                    "args": [
                        {
                            "dtype": "UINT8",
                            "name": "PreviousStateID"
                        },
                        {
                            "dtype": "UINT8",
                            "name": "CurrentStateID"
                        }
                    ]
                }
            ],
            "properties": [
                {
                    "id": 16,
                    "name": "uC_DEVID",
                    "dtype": "UINT32",
                    "ro": true,
                    "doc": "32bit Device-ID of STM32 microcontroller."
                },
                {
                    "id": 17,
                    "name": "uC_UID",
                    "dtype": "BLOB",
                    "ro": true,
                    "doc": "96bit unique-ID of STM32 microcontroller."
                },
                {
                    "id": 18,
                    "name": "LedBlinkingRate",
                    "dtype": "UINT8",
                    "ro": false,
                    "doc": "Blinking frequency of the LED given in Herz."
                },
                {
                    "id": 240,
                    "name": "LogEventThreshold",
                    "dtype": "UINT8",
                    "ro": false,
                    "doc": "Suppresses LogEvents with lower log-levels."
                },
                {
                    "id": 241,
                    "name": "FeatureState",
                    "dtype": "UINT8",
                    "ro": true,
                    "doc": "Current feature-state"
                }
            ]
        }
    ]
}